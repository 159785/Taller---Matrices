#include <stdio.h>

// Función para imprimir los elementos de las esquinas de una matriz
void imprimirEsquinas(int matriz[][100], int filas, int columnas) {
    // Esquina superior izquierda
    printf("Esquina superior izquierda: %d\n", matriz[0][0]);
    
    // Esquina superior derecha
    printf("Esquina superior derecha: %d\n", matriz[0][columnas-1]);
    
    // Esquina inferior izquierda
    printf("Esquina inferior izquierda: %d\n", matriz[filas-1][0]);
    
    // Esquina inferior derecha
    printf("Esquina inferior derecha: %d\n", matriz[filas-1][columnas-1]);
}

int main() {
    int matriz[100][100];
    int filas, columnas;

    printf("Ingrese el número de filas: ");
    scanf("%d", &filas);
    printf("Ingrese el número de columnas: ");
    scanf("%d", &columnas);

    printf("Ingrese los elementos de la matriz:\n");
    for (int i = 0; i < filas; i++) {
        for (int j = 0; j < columnas; j++) {
            scanf("%d", &matriz[i][j]);
        }
    }

    imprimirEsquinas(matriz, filas, columnas);

    return 0;
}
